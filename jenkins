pipeline{
	agent any
		environment {
			PATH = "$PATH:/opt/apache-maven-3.6.3/bin"
		}
		stages{
			stage('Get Code'){
				steps{
					git 'https://github.com/Priyankar31/Swara-Maven.git'
				}
			}
			stage('Build'){
				steps{
					sh 'mvn clean package'
				}
			}
			stage('Sonarqube Analysis'){
				steps{
					withSonarQubeEnv('Sonar-Server-7.8'){
						sh 'mvn sonar:sonar'
					}
				}
			}
		}
}
